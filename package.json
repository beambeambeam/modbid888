    "start": "next start",
    "lint": "next lint",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
